#VRML 97

DEF shape1 Shape
{
  appearance Appearance
  {
    material DF mat Material                 # DF instead of DEF
    {
      diffuseColor -2e-1 -0.2 +0.71
    } # Material
  } # Appearance
  geomtry Sphere                             # geometry instead of geometry
  {
    radius 1.2
  } # Sphere
} # Shape

Shape                                        # no '{'
   geometry Bx {}                            # Bx instead of Box
}

Shape {
   appearance    DEF app_1 Appearance {
      material USE mat
   }
   geometry Material {                       # Type mismatch
        string [ "VRML" "XML" ]
        length  [ 3.5, 3.0 ]
                                             # no '}'
}

USE shap1